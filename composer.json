{
    "name": "goodcatch/laravel-modules",
    "description": "Laravel Module management extensions",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "allen",
            "email": "ali@goodcatch.cn",
            "homepage": "https://www.goodcatch.cn/laravel-modules",
            "role": "Developer"
        }
    ],

    "require": {
        "php": ">=7.2.5",
        "ext-json": "*",
        "nwidart/laravel-modules": "^7.1"
    },
    "require-dev": {
        "illuminate/support": "^6.0",
        "laravel/framework": "^6.0",
        "laravel/helpers": "^1.1",
        "laravel/laravel": "^6.0",
        "mcamara/laravel-localization": "^1.5"
    },
    "autoload": {
        "psr-4": {
            "Goodcatch\\Modules\\": "src"

        },
        "files": [
            "src/helpers.php"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "Goodcatch\\Modules\\LaravelModulesServiceProvider"
            ],
            "aliases": {
                "GoodcatchModule": "Goodcatch\\Modules\\Facades\\Module",
                "modules.service.permission": "Goodcatch\\Modules\\Laravel\\Contracts\\Auth\\ModulePermissionService"
            },
            "dont-discover": [
                "nwidart/laravel-modules"
            ]

        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
