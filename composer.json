{
    "name": "goodcatch/laravel-modules",
    "description": "Laravel Modules development",
    "version": "0.1.0",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "allen",
            "email": "ali@goodcatch.cn",
            "homepage": "http://note.goodcatch.cn/it/dev-laravel/laravel-modules/laravel-modules",
            "role": "Developer"
        }
    ],

    "require": {
        "php": ">=7.3",
        "ext-json": "*",
        "nwidart/laravel-modules": "^8.0"
    },
    "require-dev": {
        "illuminate/support": "^8.0",
        "laravel/framework": "^8.0",
        "elendev/nexus-composer-push": "^0.4.0",
        "laravel/helpers": "^1.1"
    },
    "autoload": {
        "psr-4": {
            "Goodcatch\\Modules\\": "src"

        },
        "files": [
            "src/helpers.php"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "Goodcatch\\Modules\\LaravelModulesServiceProvider"
            ],
            "aliases": {
                "GoodcatchModule": "Goodcatch\\Modules\\Facades\\Module"
            },
            "dont-discover": [
                "nwidart/laravel-modules"
            ]

        },
        "nexus-push": {
            "url": "https://repos.goodcatch.cn/repository/composer",
            "ignore-by-git-attributes": true,
            "ignore": [
                "test.php",
                "auth.json",
                ".git",
                ".DS_Store",
                ".idea/",
                "/vendor"
            ]
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://repos.goodcatch.cn/repository/goodcatch-composer-group/"
        },
        {
            "packagist.org": false
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true
}
